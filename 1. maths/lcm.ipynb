    "def gcd(m,n):\n",
    "    if(m<n):\n",
    "        m,n=n,m\n",
    "    while(m%n!=0):\n",
    "        m,n=n,m%n\n",
    "    return n\n",
    "lcm=(15*2)//gcd(15,2)"
   ]

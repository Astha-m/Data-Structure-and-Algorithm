   "source": [
    "def power(x,n):\n",
    "    if(n==0):\n",
    "        return 1\n",
    "    if(n<0):\n",
    "        return 1/power(x,-n)\n",
    "    \n",
    "    res=power(x,n//2)\n",
    "    temp=res*res\n",
    "    if(n%2==0):\n",
    "        return temp\n",
    "    else:\n",
    "        return x*temp\n",
    "    \n",
    "power(2,10)"
   ]

    "def prime_factors(n):\n",
    "    if(n<=1):\n",
    "        return -1\n",
    "    i=2\n",
    "    while(i*i<=n):\n",
    "        while(n%i==0):\n",
    "            print(i)\n",
    "            n//=i\n",
    "        i+=1\n",
    "    if(n>1):\n",
    "        return n\n",
    "    \n",
    "prime_factors(100)"
   ]

   "source": [
    "def reverse(queue):\n",
    "    if(len(queue)>0):\n",
    "        item=queue.pop(0)\n",
    "        \n",
    "        reverse(queue)\n",
    "        queue.append(item)\n",
    "        return queue"
   ]

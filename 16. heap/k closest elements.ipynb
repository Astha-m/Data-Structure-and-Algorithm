   "source": [
    "def k_closest_elements(arr,k,x):\n",
    "    heap=[]\n",
    "    import heapq\n",
    "    for i in arr:\n",
    "        heapq.heappush(heap,(abs(i-x),i))\n",
    "    \n",
    "    for i in range(k):\n",
    "        print(heapq.heappop(heap)[1])"
   ]
  },

   "source": [
    "def k_smallest(arr,k):\n",
    "    heap=[]\n",
    "    import heapq\n",
    "    for i in arr:\n",
    "        heapq.heappush(heap,i)\n",
    "    \n",
    "    for i in range(k):\n",
    "        print(heapq.heappop(heap))"
   ]

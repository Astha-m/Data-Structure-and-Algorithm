    "def permutation(strg,i):\n",
    "    if(i==len(strg)-1):\n",
    "        print(strg)\n",
    "        return\n",
    "    for j in range(i,len(strg)):\n",
    "        strg[i],strg[j]=strg[j],strg[i]\n",
    "        permutation(strg,i+1)\n",
    "        strg[i],strg[j]=strg[j],strg[i]\n",
    "    "
   ]

   "source": [
    "arr=[1,0,1,1,1,1,0,1,1]\n",
    "\n",
    "maxi=0\n",
    "start=-1\n",
    "end=-1\n",
    "for i in range(len(arr)):\n",
    "    if(arr[i]==1):\n",
    "        if(start==-1):\n",
    "            start=i\n",
    "        else:\n",
    "            end=i\n",
    "    else:\n",
    "        maxi=max(maxi,end-start+1)\n",
    "        start=-1\n",
    "        end=-1\n",
    "maxi=max(maxi,end-start+1)\n",
    "print(maxi)"
   ]

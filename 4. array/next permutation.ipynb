   "source": [
    "def reverse(arr,start,end):\n",
    "    while(start<end):\n",
    "        arr[start],arr[end]=arr[end],arr[start]\n",
    "        start+=1\n",
    "        end-=1"
    "    \n",
    "\n",
    "def next_permutation(arr):\n",
    "    ind1=-1\n",
    "    \n",
    "    n=len(arr)\n",
    "    \n",
    "    for i in range(n-2,-1,-1):\n",
    "        if(arr[i]<arr[i+1]):\n",
    "            ind1=i\n",
    "            break\n",
    "    if(ind1==-1):\n",
    "        return sorted(arr)\n",
    "    ind2=-1\n",
    "    for i in range(n-1,-1,-1):\n",
    "        if(arr[i]>arr[ind1]):\n",
    "            ind2=i\n",
    "            break\n",
    "    arr[ind1],arr[ind2]=arr[ind2],arr[ind1]\n",
    "    reverse(arr,ind1+1,n-1)\n",
    "    return arr"
   ]
  },

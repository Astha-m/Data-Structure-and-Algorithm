   "source": [
    "def hoare(arr):\n",
    "    pivot=arr[0]\n",
    "    ind=0\n",
    "    \n",
    "    for i in range(len(arr)):\n",
    "        if(arr[i]<pivot):\n",
    "            arr[ind],arr[i]=arr[i],arr[ind]\n",
    "            ind+=1\n",
    "    print(arr)\n",
    "    return ind-1"
   ]

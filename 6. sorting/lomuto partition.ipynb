   "source": [
    "def lomuto(arr):\n",
    "    pivot=arr[-1]\n",
    "    ind=0\n",
    "    for i in range(len(arr)):\n",
    "        if(arr[i]<pivot):\n",
    "            arr[ind],arr[i]=arr[i],arr[ind]\n",
    "            ind+=1\n",
    "    arr[ind],arr[-1]=arr[-1],arr[ind]\n",
    "    print(arr)\n",
    "    return ind"
   ]

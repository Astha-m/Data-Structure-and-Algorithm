   "source": [
    "def selection_sort(arr):\n",
    "    n=len(arr)\n",
    "    \n",
    "    for i in range(n-1):\n",
    "        mini=i\n",
    "        for j in range(i+1,n):\n",
    "            if(arr[j]<arr[mini]):\n",
    "                mini=j\n",
    "        arr[mini],arr[i]=arr[i],arr[mini]\n",
    "    return arr\n",
    "\n",
    "selection_sort([9,8,7,6,5,4,3,2,1])"
   ]

   "source": [
    "from collections import defaultdict\n",
    "def count_distinct(arr):\n",
    "    d=defaultdict(int)\n",
    "    for i in arr:\n",
    "        d[i]+=1\n",
    "    return len(d)\n",
    "\n",
    "count_distinct([15,12,13,12,13,13,18])"
   ]

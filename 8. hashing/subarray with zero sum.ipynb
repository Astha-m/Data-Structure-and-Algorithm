    "\n",
    "def subarray_0_sum(arr):\n",
    "    d=set()\n",
    "    prefix_sum=0\n",
    "    for i in arr:\n",
    "        prefix_sum+=i\n",
    "        if(prefix_sum==0):\n",
    "            return True\n",
    "        if(prefix_sum-0 in d):\n",
    "            return True\n",
    "        else:\n",
    "            d.add(prefix_sum)\n",
    "subarray_0_sum([3,4,3,-1,1])"
   ]

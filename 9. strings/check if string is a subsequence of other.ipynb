    "def check_subsequence(str1,str2):\n",
    "    m=len(str1)\n",
    "    n=len(str2)\n",
    "    i=0\n",
    "    j=0\n",
    "    \n",
    "    while(i<m and j<n):\n",
    "        if(str1[i]==str2[j]):\n",
    "            j+=1\n",
    "        i+=1\n",
    "    return j==n\n",
    "\n",
    "check_subsequence('abcd','ad')"
   ]

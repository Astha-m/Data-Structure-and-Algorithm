   "source": [
    "coins=[9,6,5,1]\n",
    "val=11\n",
    "n=len(coins)\n",
    "dp=[float('inf')]*(val+1)\n",
    "dp[0]=0\n",
    "for i in range(1,val+1):\n",
    "    for j in range(n):\n",
    "        if(coins[j]<=i):\n",
    "            subres=dp[i-coins[j]]\n",
    "            if(subres!=float('inf')):\n",
    "                dp[i]=min(dp[i],subres+1)\n",
    "print(dp[-1])"
   ]

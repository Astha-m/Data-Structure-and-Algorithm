   "source": [
    "def combination(n,r):\n",
    "    if(r>n-r):\n",
    "        r=n-r\n",
    "    \n",
    "    res=1\n",
    "    for i in range(r):\n",
    "        res=res*(n-i)\n",
    "        res=res//(i+1)\n",
    "    return res"
   ]
